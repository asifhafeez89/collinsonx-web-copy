{
  "name": "@collinsonx/pages",
  "version": "0.0.1",
  "repository": "https://github.com/vercel/examples.git",
  "license": "MIT",
  "scripts": {
    "dev": "pnpm build:swc -w",
    "build": "rm -rf dist && pnpm build:swc && pnpm types",
    "build:swc": "swc src -d dist",
    "types": "tsc --emitDeclarationOnly",
    "lint": "TIMING=1 eslint \"**/*.{ts,tsx,js,jsx}\" --fix",
    "clean": "rm -rf ./dist && rm -rf .turbo"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./*": [
      "./dist/*.js",
      "./dist/*/index.js"
    ]
  },
  "files": [
    "dist"
  ],
  "typesVersions": {
    "*": {
      "*": [
        "src/*",
        "src/*/index"
      ]
    }
  },
  "publishConfig": {
    "typesVersions": {
      "*": {
        "*": [
          "dist/*.d.ts",
          "dist/*/index.d.ts"
        ]
      }
    }
  },
  "dependencies": {
    "@collinsonx/design-system": "workspace:*",
    "@collinsonx/utils": "workspace:*",
    "apollo-server-micro": "^3.11.1"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.10",
    "chokidar": "^3.5.3",
    "eslint": "^8.16.0",
    "eslint-config-collinsonx": "workspace:*",
    "typescript": "^4.9.4"
  }
}
