{
  "name": "@collinsonx/utils",
  "version": "0.0.1",
  "repository": "https://github.com/vercel/examples.git",
  "license": "MIT",
  "scripts": {
    "dev": "pnpm build --watch",
    "build": "tsup src/** --format esm,cjs --dts --external react",
    "lint": "TIMING=1 eslint \"**/*.{ts,tsx,js,jsx}\" --fix",
    "clean": "rm -rf dist && rm -rf .turbo",
    "gql:compile": "graphql-codegen-esm -v",
    "gql:watch": "graphql-codegen-esm -w"
  },
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.js",
    "./*": [
      "./dist/*.js",
      "./dist/*/index.js"
    ]
  },
  "typesVersions": {
    "*": {
      "*": [
        "src/*",
        "src/*/index"
      ]
    }
  },
  "publishConfig": {
    "typesVersions": {
      "*": {
        "*": [
          "dist/*.d.ts",
          "dist/*/index.d.ts"
        ]
      }
    }
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "dayjs": "^1.11.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@graphql-codegen/cli": "^3.0.0",
    "@graphql-codegen/client-preset": "^2.0.0",
    "@graphql-typed-document-node/core": "^3.1.1",
    "@types/node": "^18.13.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "dayjs": "^1.11.7",
    "eslint": "^8.16.0",
    "eslint-config-collinsonx": "workspace:*",
    "jest-environment-jsdom": "^29.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^5.12.8",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@apollo/client": "^3.7.6",
    "@apollo/link-error": "2.0.0-beta.3",
    "graphql": "^16.6.0",
    "supertokens-auth-react": "0.31.1",
    "supertokens-web-js": "^0.5.0"
  }
}
