{
  "name": "@collinsonx/utils",
  "version": "0.0.1",
  "repository": "https://github.com/vercel/examples.git",
  "license": "MIT",
  "scripts": {
    "dev": "pnpm build --watch",
    "build": "tsup src/** --format esm,cjs --dts --external react",
    "lint": "TIMING=1 eslint \"**/*.{ts,tsx,js,jsx}\" --fix",
    "clean": "rm -rf dist && rm -rf .turbo",
    "gql:compile": "graphql-codegen-esm -v",
    "gql:watch": "graphql-codegen-esm -w",
    "test": "jest"
  },
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.js",
    "./*": [
      "./dist/*.js",
      "./dist/*/index.js"
    ]
  },
  "typesVersions": {
    "*": {
      "*": [
        "src/*",
        "src/*/index"
      ]
    }
  },
  "publishConfig": {
    "typesVersions": {
      "*": {
        "*": [
          "dist/*.d.ts",
          "dist/*/index.d.ts"
        ]
      }
    }
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "dayjs": "^1.11.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/validator": "^13.11.6",
    "dayjs": "^1.11.10",
    "eslint": "^8.53.0",
    "eslint-config-collinsonx": "workspace:*",
    "jest-environment-jsdom": "^29.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@apollo/client": "^3.8.7",
    "@apollo/link-error": "2.0.0-beta.3",
    "@types/lodash": "^4.14.201",
    "deepmerge": "^4.3.1",
    "graphql": "^16.8.1",
    "lodash": "^4.17.21",
    "supertokens-auth-react": "0.34.0",
    "supertokens-web-js": "0.7.0",
    "validator": "^13.11.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
