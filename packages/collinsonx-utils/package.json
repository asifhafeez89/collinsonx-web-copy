{
  "name": "@collinsonx/utils",
  "version": "0.0.1",
  "repository": "https://github.com/vercel/examples.git",
  "license": "MIT",
  "scripts": {
    "dev": "pnpm build --watch",
    "build": "tsup src/*/** --format esm,cjs --dts --external react",
    "lint": "TIMING=1 eslint \"**/*.{ts,tsx,js,jsx}\" --fix",
    "clean": "rm -rf dist && rm -rf .turbo",
    "gql:compile": "graphql-codegen-esm",
    "gql:watch": "graphql-codegen-esm -w"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./*": [
      "./dist/*.js",
      "./dist/*/index.js"
    ]
  },
  "typesVersions": {
    "*": {
      "*": [
        "src/*",
        "src/*/index"
      ]
    }
  },
  "publishConfig": {
    "typesVersions": {
      "*": {
        "*": [
          "dist/*.d.ts",
          "dist/*/index.d.ts"
        ]
      }
    }
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^3.0.0",
    "@graphql-codegen/client-preset": "^2.0.0",
    "@graphql-typed-document-node/core": "^3.1.1",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "eslint": "^8.16.0",
    "eslint-config-collinsonx": "workspace:*",
    "tsup": "^5.12.8",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@apollo/client": "^3.7.6",
    "@apollo/link-error": "2.0.0-beta.3",
    "graphql": "^16.6.0",
    "supertokens-auth-react": "0.28.1"
  }
}
