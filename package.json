{
  "repository": "https://github.com/vercel/examples.git",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "build:main": "turbo run build --filter=main...",
    "build:search": "turbo run build --filter=search...",
    "build:lounge": "turbo run build --filter=lounge...",
    "build:partner-management": "turbo run build --filter=partner-management...",
    "e2e:dev": "turbo run e2e:dev",
    "e2e:start": "turbo run e2e:start",
    "dev": "turbo run dev",
    "start": "turbo run start",
    "clean": "turbo run clean && rm -rf node_modules",
    "lint": "turbo run lint",
    "format": "prettier --write .",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "turbo run build --filter=main... && changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.23.0",
    "@types/node": "^17.0.14",
    "prettier": "^2.6.2",
    "turbo": "latest"
  },
  "packageManager": "pnpm@7.15.0"
}
